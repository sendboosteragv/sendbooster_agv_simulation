<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="sendbooster_agv">
        <link name="base_footprint"/>

    <!-- Base Link -->
    <link name="base_link">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <iyy>0.1</iyy>
          <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name="base_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.4 0.4 0.2</size>
          </box>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.8 1</ambient>
          <diffuse>0.2 0.2 0.8 1</diffuse>
        </material>
      </visual>
      <collision name="base_collision">
        <geometry>
          <box>
            <size>0.4 0.4 0.2</size>
          </box>
        </geometry>
      </collision>
    </link>
    <joint name="base_joint" type="fixed">
      <parent>base_footprint</parent>
      <child>base_link</child>
      <pose>0.0 0.0 0.2465 0 0 0</pose>
    </joint>
    <!-- Left Wheel -->
  <link name="wheel_left_link">
    <pose>0 0.16 -0.035 1.5708 0 0</pose>

    <inertial>
      <mass>0.5</mass>
      <inertia>
        <ixx>0.001</ixx>
        <iyy>0.001</iyy>
        <izz>0.001</izz>
      </inertia>
    </inertial>

    <visual name="wheel_left_visual">
      <geometry>
        <cylinder>
          <radius>0.0965</radius>
          <length>0.048</length>
        </cylinder>
      </geometry>
      <material>
        <ambient>0 0 0 1</ambient>
        <diffuse>0 0 0 1</diffuse>
      </material>
    </visual>

    <collision name="wheel_left_collision">
      <geometry>
        <cylinder>
          <radius>0.0965</radius>
          <length>0.048</length>
        </cylinder>
      </geometry>
    </collision>
  </link>
<link name="caster_front_link">
  <pose>-0.28 0 -0.1115 0 0 0</pose>
  <inertial>
    <mass>0.2</mass>
    <inertia>
      <ixx>0.0005</ixx>
      <iyy>0.0005</iyy>
      <izz>0.0005</izz>
    </inertia>
  </inertial>

  <collision name="caster_collision">
    <geometry>
      <sphere>
        <radius>0.0235</radius>
      </sphere>
    </geometry>
    <surface>
      <friction>
        <ode>
          <mu>0.0</mu>
          <mu2>0.0</mu2>
        </ode>
      </friction>
    </surface>
  </collision>

  <visual name="caster_visual">
    <geometry>
      <sphere>
        <radius>0.0235</radius>
      </sphere>
    </geometry>
  </visual>
</link>

<joint name="caster_front_joint" type="fixed">
  <parent>base_link</parent>
  <child>caster_front_link</child>
</joint>

<link name="caster_back_link">
  <pose>0.28 0 -0.1115 0 0 0</pose>
  <inertial>
    <mass>0.2</mass>
    <inertia>
      <ixx>0.0005</ixx>
      <iyy>0.0005</iyy>
      <izz>0.0005</izz>
    </inertia>
  </inertial>

  <collision name="caster_collision">
    <geometry>
      <sphere>
        <radius>0.0235</radius>
      </sphere>
    </geometry>
    <surface>
      <friction>
        <ode>
          <mu>0.0</mu>
          <mu2>0.0</mu2>
        </ode>
      </friction>
    </surface>
  </collision>

  <visual name="caster_visual">
    <geometry>
      <sphere>
        <radius>0.0235</radius>
      </sphere>
    </geometry>
  </visual>
</link>

<joint name="caster_back_joint" type="fixed">
  <parent>base_link</parent>
  <child>caster_back_link</child>
</joint>

    <!-- Right Wheel -->
    <link name="wheel_right_link">
      <pose>0 -0.16 -0.035 1.5708 0 0</pose>

      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.001</ixx>
          <iyy>0.001</iyy>
          <izz>0.001</izz>
        </inertia>
      </inertial>

      <visual name="wheel_right_visual">
        <geometry>
          <cylinder>
            <radius>0.0965</radius>
            <length>0.048</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>

      <collision name="wheel_right_collision">
        <geometry>
          <cylinder>
            <radius>0.0965</radius>
            <length>0.048</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Wheel Joints -->
    <joint name="wheel_left_joint" type="revolute">
      <parent>base_link</parent>
      <child>wheel_left_link</child>
      <axis>
        <xyz>0 0 -1</xyz>
        <limit>
          <lower>-1000</lower>
          <upper>1000</upper>
        </limit>
      </axis>
    </joint>

    <joint name="wheel_right_joint" type="revolute">
      <parent>base_link</parent>
      <child>wheel_right_link</child>
      <axis>
        <xyz>0 0 -1</xyz>
        <limit>
          <lower>-1000</lower>
          <upper>1000</upper>
        </limit>
      </axis>
    </joint>

    <!-- IMU -->
    <link name="imu_link">
      <pose>0 0 0.1 0 0 0</pose>
      <sensor name="sendbooster_imu" type="imu">
        <always_on>true</always_on>
        <update_rate>100</update_rate>
        <imu>
          <angular_velocity>
            <x>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>2e-4</stddev>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>2e-4</stddev>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>2e-4</stddev>
              </noise>
            </z>
          </angular_velocity>
          <linear_acceleration>
            <x>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>1.7e-2</stddev>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>1.7e-2</stddev>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>1.7e-2</stddev>
              </noise>
            </z>
          </linear_acceleration>
        </imu>
        <plugin name="sendbooster_imu_plugin" filename="libgazebo_ros_imu_sensor.so">
          <ros>
            <remapping>~/out:=imu</remapping>
          </ros>
          <initial_orientation_as_reference>false</initial_orientation_as_reference>
          <frame_name>imu_link</frame_name>
        </plugin>
      </sensor>
    </link>
    <joint name="imu_joint" type="fixed">
      <parent>base_link</parent>
      <child>imu_link</child>
      <pose>0 0 0.1 0 0 0</pose>
    </joint>

    <!-- LiDAR -->
    <link name="base_scan">
      <pose>0.32 0 0 0 0 0</pose>
      <visual name="lidar_visual">
        <geometry>
          <cylinder>
            <radius>0.03</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0.8 1</ambient>
          <diffuse>0 0 0.8 1</diffuse>
        </material>
      </visual>
          <!-- RPLIDAR A2M8 Front (180° FOV) -->
          <sensor name="rplidar_a2m8_front" type="ray">
        <always_on>true</always_on>
        <visualize>true</visualize>
        <pose>0 0 0 0 0 0</pose>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>400</samples>
              <resolution>1.000000</resolution>
              <min_angle>-1.5708</min_angle>
              <max_angle>1.5708</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>12.0</max>
            <resolution>0.005</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="sendbooster_agv_laserscan" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <!-- <namespace>/tb3</namespace> -->
            <remapping>~/out:=scan</remapping>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
          <frame_name>base_scan</frame_name>
        </plugin>
      </sensor>
</link>
    <joint name="lidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>base_scan</child>
      <pose>0.2 0 0.15 0 0 0</pose>
    </joint>



    <link name="base_scan_back">
      <pose>-0.32 0 0 0 0 0</pose>
      <visual name="lidar_visual_back">
        <geometry>
          <cylinder>
            <radius>0.03</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0.8 1</ambient>
          <diffuse>0 0 0.8 1</diffuse>
        </material>
      </visual>
          <!-- RPLIDAR A2M8 Back (180° FOV, rotated to face backward) -->
          <sensor name="rplidar_a2m8_back" type="ray">
        <always_on>true</always_on>
        <visualize>true</visualize>
        <pose>0 0 0 0 0 3.14159</pose>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>400</samples>
              <resolution>1.000000</resolution>
              <min_angle>-1.5708</min_angle>
              <max_angle>1.5708</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>12.0</max>
            <resolution>0.005</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="sendbooster_agv_laserscan_back" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <!-- <namespace>/tb3</namespace> -->
            <remapping>~/out:=scan2</remapping>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
          <frame_name>base_scan_back</frame_name>
        </plugin>
      </sensor>
</link>
    <joint name="lidar_joint_back" type="fixed">
      <parent>base_link</parent>
      <child>base_scan_back</child>
      <pose>0.2 0 0.15 0 0 0</pose>
    </joint>
    <!-- Differential Drive Plugin -->
    <plugin name="sendbooster_diff_drive" filename="libgazebo_ros_diff_drive.so">
      <ros>
        <!-- <namespace>/tb3</namespace> -->
      </ros>

      <update_rate>30</update_rate>

      <!-- wheels -->
      <left_joint>wheel_left_joint</left_joint>
      <right_joint>wheel_right_joint</right_joint>

      <wheel_separation>0.064</wheel_separation>
      <wheel_diameter>0.193</wheel_diameter>

<!-- limits -->
      <max_wheel_torque>20</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>

      <command_topic>cmd_vel</command_topic>

      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>false</publish_odom_tf>  <!-- EKF will publish odom->base_footprint TF -->
      <publish_wheel_tf>false</publish_wheel_tf>

      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_footprint</robot_base_frame>
    </plugin>
    <plugin name="sendbooster_joint_state" filename="libgazebo_ros_joint_state_publisher.so">
      <ros>
        <!-- <namespace>/tb3</namespace> -->
        <remapping>~/out:=joint_states</remapping>
      </ros>
      <update_rate>30</update_rate>
      <joint_name>wheel_left_joint</joint_name>
      <joint_name>wheel_right_joint</joint_name>
    </plugin>    
  </model>
</sdf>
